{
  "name": "@io-maana/q-assistant-client",
  "version": "3.3.0-beta.20",
  "description": "JavaScript package to streamline communication between an assistant and the Maana Q Assistant API.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "exports": {
    ".": "./build/"
  },
  "scripts": {
    "build": "npm run build:ts && npm run build:doc",
    "build:ts": "tsc --build tsconfig.json",
    "build:doc": "typedoc",
    "prepack": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"src/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maana-io/q-assistant-client"
  },
  "keywords": [
    "Maana",
    "Q"
  ],
  "author": "Logan Gore @ Maana.io",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maana-io/q-assistant-client/issues"
  },
  "homepage": "https://github.com/maana-io/q-assistant-client#readme",
  "dependencies": {
    "@io-maana/typesystem-utils": "^1.0.9",
    "post-robot": "10.0.29"
  },
  "devDependencies": {
    "@types/node": "^14.18.33",
    "@types/post-robot": "^10.0.0",
    "prettier": "^2.7.1",
    "typedoc": "^0.23.21",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "^4.8.4"
  }
}
