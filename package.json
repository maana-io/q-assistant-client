{
  "name": "@io-maana/q-assistant-client",
  "version": "3.2.3-beta",
  "description": "JavaScript package to streamline communication between an assistant and the Maana Q Assistant API.",
  "main": "build/index.js",
  "exports": {
    "./": "./build/"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:tsd",
    "build:js": "babel src --out-dir build",
    "build:tsd": "tsc --build tsconfig.json",
    "clean": "rimraf build",
    "prepack": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"src/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maana-io/q-assistant-client"
  },
  "keywords": [
    "Maana",
    "Q"
  ],
  "author": "Logan Gore @ Maana.io",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maana-io/q-assistant-client/issues"
  },
  "homepage": "https://github.com/maana-io/q-assistant-client#readme",
  "dependencies": {
    "core-js": "3.6.4",
    "post-robot": "10.0.29"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "prettier": "1.19.1",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.5"
  }
}
